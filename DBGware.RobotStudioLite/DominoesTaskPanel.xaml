<UserControl x:Class="DBGware.RobotStudioLite.DominoesTaskPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:DBGware.RobotStudioLite"
             mc:Ignorable="d"
             d:DesignWidth="800" d:DesignHeight="450">
    <Grid Margin="8">
        <TabControl TabStripPlacement="Left">
            <TabItem Header="{DynamicResource Settings}" MinWidth="100">
                <local:DominoesTaskPanelSettingsTab x:Name="dominoesTaskPanelSettingsTab" />
            </TabItem>
            <TabItem Header="{DynamicResource Calibration}" MinWidth="100">
                <local:DominoesTaskPanelCalibrationTab x:Name="dominoesTaskPanelCalibrationTab" />
            </TabItem>
            <!--<TabItem Header="{DynamicResource Test}" MinWidth="100">

            </TabItem>-->
        </TabControl>
    </Grid>
</UserControl>